{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "Discord bot built in Typescript",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/src/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/src/index.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "npm test"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "discord",
    "bot"
  ],
  "author": "Nicol√°s Flores Montenegro",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.4",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
